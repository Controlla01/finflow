Module Module1

    Sub Main()

        Dim no1, no2, result As Double

        Console.WriteLine("THIS SOFTWARE PERFORMS THE FRACTION OF TWO NUMBERS")
        Console.WriteLine("=====================================================")

        Console.WriteLine("")

        Console.WriteLine("Input the First Number")
        no1 = Console.ReadLine()

        Console.WriteLine("")

        Console.WriteLine("Input the Second Number")
        no2 = Console.ReadLine()

        result = no1 / no2

        Console.WriteLine("")

        Console.WriteLine("Result = " + result.ToString)

        Console.ReadLine()

    End Sub

End Module
